{
  "presets": ["@babel/env", "@babel/react", "@babel/preset-typescript"],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@components": "./components",
          "@action": "./core/action",
          "@utils": "./core/utils",
          "@assets": "./assets",
          "@pages": "./pages"
        }
      }
    ]
  ]
}
